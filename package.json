  "files": [
    "lib/!(__tests__)",
    "lib/!(__tests__)/**/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@jest/globals": "^29.5.0",
    "@types/node": "^16.18.14",
    "jest": "^29.5.0",
    "jest-mock": "^29.5.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.0.5",
    "typescript": "~5.2.0"
  },
